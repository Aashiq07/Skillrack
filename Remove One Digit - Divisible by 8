import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc= new Scanner(System.in);
		String n=sc.next();
		boolean f=true;
		for(int i=0;i<n.length();i++)
		{
		    String s=n.substring(0,i)+n.substring(i+1);
		    int fval=Integer.parseInt(s);
		    if(fval%8==0 && fval!=0)
		    {
		        f=false;
		    }
		}
		System.out.print(!f?"YES":"NO");
	}
}
