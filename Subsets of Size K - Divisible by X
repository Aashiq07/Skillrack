import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc= new Scanner(System.in);
		String n=sc.next();
		int k=sc.nextInt(),c=0;
		int x=sc.nextInt();
		for(int i=0;i<n.length()-k+1;i++)
		{
		    String val=n.substring(i,i+k);
		    if(Integer.parseInt(val)%x==0 && val.charAt(0)!='0')
		    c++;
		}
		System.out.print(c);
	}
}
