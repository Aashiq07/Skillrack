import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc= new Scanner(System.in);
		int n=sc.nextInt();
		int arr[]=new int[n];
		for(int i=0;i<n;i++)
		{
		    arr[i]=sc.nextInt();
		}
		int max=0;
		for(int i=0;i<n;i++)
		{
		    if(max<arr[i])
		    {
		        max=arr[i];
		    }
		}
		for(int i=0;i<n;i++)
		{
		    System.out.print(max-arr[i]+" ");
		}

	}
}
