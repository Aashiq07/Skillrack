import java.util.*;
public class Hello {

    public static void main(String[] args) {
		//Your Code Here
		Scanner sc= new Scanner(System.in);
		int n=sc.nextInt(),sum=0;
		int arr[]= new int[n];
		for(int i=0;i<n;i++)
		arr[i]=sc.nextInt();
		int d=sc.nextInt();
		char v=(char)(d+'0');
		for(int i=0;i<n;i++)
		{
		    String val=String.valueOf(arr[i]);
		    if(val.charAt(0)==v)
		    {
		        sum+=arr[i];
		    }
		}
		System.out.print(sum);

	}
}
